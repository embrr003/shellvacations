<script type="text/javascript">

function popUp(){
	var memberType = document.forms['resortFlow2a'].elements['memberType'].value;
	if(memberType == 'explorer'|| memberType == 'navigator'){

	 var confirmBox = confirm('Your loan must be paid in full 14 days prior to arrival or your reservation will be cancelled and points forfeited.');
	  if(confirmBox){
	  	document.forms['resortFlow2a'].submit();
	  	openSearchBox();
	  }else
		  document.forms['resortFlow2a'].elements['Submit'].disabled=false;
	}
	else{
		document.forms['resortFlow2a'].submit();
	}	

}

function openSearchBox(){
	document.getElementById("sBox").style.display="block";
}

function reloadArriveDate(y,m,d){
	populateDropDowns(document.resortFlow2a.arrive_year, y);
	populateDropDowns(document.resortFlow2a.arrive_month, m);
	populateDropDowns(document.resortFlow2a.arrive_day, d);
}

function reloadDepartDate(y,m,d){
	populateDropDowns(document.resortFlow2a.depart_year, y);
	populateDropDowns(document.resortFlow2a.depart_month, m);
	populateDropDowns(document.resortFlow2a.depart_day, d);
}
</script>
