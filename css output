			                 today4.setDate(today4.getDate()+day1);
			                 cal1x.currentDate = today4;
			                 cal1x.addDisabledDates(null, formatDate(today3,"yyyy-MM-dd"));
			                 cal1x.setReturnFunction('reloadArriveDate');
			                 cal1x.todayText = "";
			                </SCRIPT> 
                            <DIV ID="div1" STYLE="position:absolute;visibility:hidden;background-color:white;layer-background-color:white;"></DIV>
